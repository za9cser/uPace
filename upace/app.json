{
  "expo": {
    "name": "uPace",
    "slug": "upace",
    "version": "1.3.2",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon-dark.png",
    "scheme": "upace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.za9cser.upaceapp",
      "versionCode": 15
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a850d7f-7982-4092-a4d4-54fafeb9f961"
      }
    },
    "owner": "za9cser",
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000",
          "resizeMode": "contain",
          "image": "./assets/images/splash-icon-dark.png",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#000"
          }
        }
      ]
    ]
  }
}
